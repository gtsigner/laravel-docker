{
  "name": "zhaojunlike/laravel-apollo-client",
  "description": "Apollo agent for laravel.",
  "license": "MIT",
  "authors": [
    {
      "name": "lackhurt",
      "email": "pangjn@yeah.net"
    },
    {
      "name": "godtoy",
      "email": "zhaojunlike@gmail.com"
    }
  ],
  "require": {
    "zhaojunlike/apollo-php-client": "^0.0.1",
    "illuminate/support": "5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.7.x",
    "illuminate/console": "5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.7.x"
  },
  "autoload": {
    "psr-4": {
      "Lackhurt\\Apollo\\": "src/"
